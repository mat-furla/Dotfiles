!! GENERAL
urxvt*cursorBlink:      false
urxvt*scrollBar:        false
urxvt*depth:            32
urxvt*internalBorder:   8
urxvt*dynamicColors:    on
urxvt*perl-ext-common:  default,matcher,selection-to-clipboard
URxvt.url-launcher:     /usr/bin/xdg-open
URxvt.matcher.button:   1


!! FONTS
URxvt.font:             xft:Iosevka:pixelsize=14:style=bold:antialias=true
URxvt.boldFont:         xft:Iosevka:pixelsize=14:style=bold:antialias=true
URxvt.italicFont:       xft:Iosevka:pixelsize=14:style=italic:antialias=true
URxvt.boldItalicFont:   xft:Iosevka:pixelsize=14:style=bold italic:antialias=true

Xft.antialias:          true
Xft.hinting:            true
Xft.hintstyle:          hintslight
Xft.lcdfilter:          lcddefault
Xft.rgba:               rgb


!! COLORS
