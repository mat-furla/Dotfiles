!! GENERAL
!urxvt*cursorBlink:      false
!urxvt*scrollBar:        false
!urxvt*depth:            32
!urxvt*internalBorder:   25
!urxvt*dynamicColors:    on
!urxvt*perl-ext-common:  default,matcher,selection-to-clipboard
!URxvt.url-launcher:     /usr/bin/xdg-open
!URxvt.matcher.button:   1


!! FONTS
!URxvt.font:             xft:UbuntuMono Nerd Font:pixelsize=18:style=regular:antialias=true
!URxvt.boldFont:         xft:UbuntuMono Nerd Font:pixelsize=18:style=bold:antialias=true
!URxvt.italicFont:       xft:UbuntuMono Nerd Font:pixelsize=18:style=italic:antialias=true
!URxvt.boldItalicFont:   xft:UbuntuMono Nerd Font:pixelsize=18:style=bold italic:antialias=true

Xft.antialias:          true
Xft.hinting:            true
Xft.hintstyle:          hintslight
Xft.lcdfilter:          lcddefault
Xft.dpi:                96
Xft.rgba:               rgb


